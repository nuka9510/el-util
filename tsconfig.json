{
  "compilerOptions": {
    "checkJs": true,
    "strictNullChecks": false,
    "noPropertyAccessFromIndexSignature": true,
    "noUncheckedIndexedAccess": true,
    "moduleResolution": "bundler",
    "useDefineForClassFields": true,
    "lib": ["ESNext", "DOM", "DOM.Iterable"],
    "target": "ESNext",
    "declaration": true,
    "declarationDir": "dist",
    "outDir": "dist",
    "baseUrl": ".",
    "paths": {
      "@nuka9510/el-util": ["src/index.ts"],
      "@nuka9510/el-util/plugin": ["src/plugin/index.ts"],
      "@nuka9510/el-util/@types/common": ["@types/common.d.ts"],
      "@nuka9510/el-util/@types/interceptor": ["@types/interceptor.d.ts"],
      "@nuka9510/el-util/@types/plugin": ["@types/plugin.d.ts"],
      "@nuka9510/el-util/@types/plugin/util-action": ["@types/plugin/util-action.d.ts"]
    }
  },
  "include": [
    "src/index.ts",
    "src/plugin/index.ts"
  ]
}